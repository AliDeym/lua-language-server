[GetLifeTime]
description = 'Returns the \'life time\' of the particle, how long the particle existed since its creation.\r\n\r\nThis value will always be between 0 and <page>CLuaParticle:GetDieTime</page>.\r\n\r\n\r\nIt changes automatically as time goes.\r\n\r\nIt can be manipulated using <page>CLuaParticle:SetLifeTime</page>.\r\n\r\n\r\nIf the life time of the particle will be more than <page>CLuaParticle:GetDieTime</page>, it will be removed.\r\n\r\n**Returns:** How long the particle existed, in seconds.\r\n\r\n**Scope:** Client'
[[.args]]
type = 'CLuaParticle'
[[.returns]]
type = 'number'

