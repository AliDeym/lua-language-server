[GetBounce]
description = 'Returns the \'bounciness\' of the particle.\r\n\r\n**Returns:** The \'bounciness\' of the particle\r\n\r\n2 means it will gain 100% of its previous velocity,\r\n\r\n\r\n1 means it will not lose velocity,\r\n\r\n\r\n0.5 means it will lose half of its velocity with each bounce.\r\n\r\n**Scope:** Client'
[[.args]]
type = 'CLuaParticle'
[[.returns]]
type = 'number'

[GetColor]
description = 'Returns the color of the particle.\r\n\r\n**Returns:** Red part of the color\r\n\r\n**Scope:** Client'
[[.args]]
type = 'CLuaParticle'
[[.returns]]
type = 'number'

[GetDieTime]
description = 'Returns the amount of time in seconds after which the particle will be destroyed.\r\n\r\n**Returns:** The amount of time in seconds after which the particle will be destroyed\r\n\r\n**Scope:** Client'
[[.args]]
type = 'CLuaParticle'
[[.returns]]
type = 'number'

